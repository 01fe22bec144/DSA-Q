#include<stdio.h>
void l_year(int y);
main()
{
    int y;
    printf("enter the value of y\n");
    scanf("%d",&y);
    l_year(y);        
}
void l_year(int y)
{
    if(y%400==0)
    {
        printf("It is leap year\n");
    }
    else if(y%4==0 && y%100!=0)
    {
        printf("It is leap year\n");
    }
    else
    {
        printf("It is not leap year\n");
    }
}
