#include<stdio.h>
void area_cirlce(float r);
main()
{
    float area,r;
    printf("enter the value of radius\n");
    scanf("%f",&r);
    area_cirlce(r);
}
void area_cirlce(float r)
{
    float area;
    if(r>=0)
    {
        area=3.142*r*r;
        printf("area=%f",area);
    }
    else
    {
        printf("INVALID");
    }
}
